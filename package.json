{
  "name": "@rubic/catalog-fetcher",
  "version": "0.99.4",
  "description": "Rubic Catalog fetcher",
  "main": "lib/index.js",
  "files": [
    "lib/index.js",
    "lib/catalog.schema.json"
  ],
  "dependencies": {
    "ajv": "^4.11.8",
    "comment-json": "^1.1.3",
